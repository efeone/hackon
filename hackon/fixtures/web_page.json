  "main_section_html": "<html>\n    <head>\n        <link rel=\"stylesheet\" href=\"https://cdn.jsdelivr.net/gh/habibmhamadi/multi-select-tag/dist/css/multi-select-tag.css\">\n        <script src=\"https://cdn.jsdelivr.net/gh/habibmhamadi/multi-select-tag/dist/js/multi-select-tag.js\"></script>\n    </head>\n    <body>\n        {% set doc = frappe.get_doc(\"Task\", frappe.form_dict.docname) %}\n        <form id=\"frmTask\">\n             <div class=\"row\">\n                <div class=\"row form-group\">\n                    <label for=\"task\" class=\"control-label\">Task ID</label>\n                    <input type=\"text\" autocomplete=\"off\" class=\"input-with-feedback form-control bold\" data-fieldtype=\"Data\" data-fieldname=\"task\" id=\"task\" name=\"task\" value={{doc.name}} disabled>\n                    \n                    <label for=\"project\" class=\"control-label\">Project</label>\n                    <input type=\"text\" autocomplete=\"off\" class=\"input-with-feedback form-control bold\" data-fieldtype=\"Data\" data-fieldname=\"project\" id=\"project\" name=\"project\" value={{doc.project}} disabled>\n                    \n                    <label for=\"subject\" class=\"control-label\">Subject</label>\n                    <input type=\"text\" autocomplete=\"off\" class=\"input-with-feedback form-control bold\" data-fieldtype=\"Data\" data-fieldname=\"subject\" id=\"subject\" name=\"subject\" value={{doc.subject}} disabled>\n                    \n                    <label for=\"status\" class=\"control-label\">Status</label>\n                    <input type=\"text\" autocomplete=\"off\" class=\"input-with-feedback form-control bold\" data-fieldtype=\"Data\" data-fieldname=\"status\" id=\"status\" name=\"status\" value={{doc.status}} disabled>\n                    \n                    <label for=\"description\" class=\"control-label\">Details</label>\n                    <textarea autocomplete=\"off\" class=\"input-with-feedback form-control bold\" data-fieldname=\"description\" id=\"description\" name=\"description\" value=\"\" disabled>\n                    </textarea>\n                    \n                    <label for=\"response-type\" class=\"control-label\">Response Type</label>\n                    <input type=\"text\" autocomplete=\"off\" class=\"input-with-feedback form-control bold\" data-fieldtype=\"Data\" data-fieldname=\"response-type\" id=\"response-type\" name=\"response-type\" value={{doc.response}} disabled>\n                    \n                    {% if doc.response =='Data' %}\n                        <label for=\"response-data\" class=\"control-label\">Response</label>\n                        <input type=\"text\" autocomplete=\"off\" class=\"input-with-feedback form-control bold\" data-fieldtype=\"Data\" data-fieldname=\"response-data\" id=\"response-data\" name=\"response-data\">\n                    {% endif %}\n                    \n                     {% if doc.response =='Attachment' %}\n                        <label for=\"response-attachment\" class=\"control-label\" id=\"response-attachments\">Response</label>\n                        <input type=\"file\" class=\"input-with-feedback form-control\" id=\"response-attachment\" name=\"response-attachment\" multiple>\n                    {% else %}\n                        <div style=\"display:none\">\n                            <label for=\"response_attachment\">Response</label>\n                            <input type=\"file\" id=\"response_attachment\" name=\"response_attachment\">\n                        </div>\n                    {% endif %}\n                    \n                     {% if doc.response =='URL' %}\n                        <label for=\"response-url\" class=\"control-label\">Response</label>\n                        <input type=\"url\" autocomplete=\"off\" class=\"input-with-feedback form-control bold\" data-fieldtype=\"Data\" data-fieldname=\"response-url\" id=\"response-url\" name=\"response-url\">\n                    {% endif %}\n                    \n                     {% if doc.response =='Text' %}\n                        <label for=\"response-text\" class=\"control-label\">Response</label>\n                        <input type=\"text\" autocomplete=\"off\" class=\"input-with-feedback form-control bold\" data-fieldtype=\"Data\" data-fieldname=\"response-text\" id=\"response-text\" name=\"response-text\">\n                    {% endif %}\n                    \n                    {% if doc.software_tool %}\n                        <div>\n                            <label for=\"tools\">Tools used</label>\n                            <select name=\"tools\" id=\"tools\" multiple>\n                                {% for software_tool in doc.software_tool %}\n                                    <option value=\"{{software_tool.software_tool}}\"> {{ software_tool.software_tool }}</option>\n                                {% endfor %}\n                            </select>\n                        </div>\n                    {% else %}\n                        <div style=\"display:none\">\n                            <label for=\"tools\">Tools used</label>\n                            <select name=\"tools\" id=\"tools\" multiple>\n                            </select>\n                        </div>\n                    {% endif %}\n                </div>\n            </div>\n        </form>\n        <button class=\"btn btn-primary\" id=\"btn_upload\">Save</button>\n    </body>\n</html>",
