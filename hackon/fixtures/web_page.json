  "main_section": null,
  "main_section_html": "<!DOCTYPE HTML>\n<html>\n    <head>\n        <script src=\"https://canvasjs.com/assets/script/canvasjs.min.js\"></script>\n    </head>\n    <body>\n        <div id=\"chartContainer\" style=\"height: 350px;\"></div>\n    </body>\n</html>\n\n<script>\n    window.onload = function () {\n        function get_realtime_data(){\n            `{% set selected_event = frappe.db.get_all(\"Event\", fields=['name'], filters={ 'status': 'Started' }) %}\n            {% if selected_event %}\n                {% set team_list = frappe.db.get_all(\"Team\", fields=['team_name', 'team_score'], filters = { 'event': selected_event[0].name }) %}\n            {% else %}\n                {% set team_list = [] %}\n            {% endif %}`\n            \n            var data_points = [];\n            `{% if team_list %}\n                {% for team in team_list %}`\n                    data_points.push({ label: `{{ team.team_name }}`, y: parseInt(`{{ team.team_score }}`) });\n                `{% endfor %}\n            {% endif %}`\n            \n            var chart = new CanvasJS.Chart(\"chartContainer\", {\n                title: {\n                    text: \"Hackon Score Board\"\n                },\n                axisY: {\n                    title: \"Team Score\",\n                    includeZero: true,\n                },\n                data: [{\n                    type: \"column\",    \n                    indexLabel: \"{y}\",\n                    dataPoints: data_points\n                }]\n                \n            });\n            \n            var TeamColor, yVal;\n            var dps = chart.options.data[0].dataPoints;\n                for (var i = 0; i < dps.length; i++) {\n                    y_val = dps[i].y;\n                    y_label = dps[i].label;\n                    TeamColor =  y_val >= 10 ? \"green\" : y_val < 10 ? \"blue\" : null;\n                    dps[i] = {label: y_label, y: y_val, color: TeamColor};\n            }\n            chart.options.data[0].dataPoints = dps; \n            chart.render();\n        }\n        get_realtime_data();\n        setTimeout(\"location.reload(true);\",8000);\n    }\n</script>",
