  "main_section_html": "<!DOCTYPE html>\n<html>\n    <body>\n        {% set perm_key = {'has_role': 0 } %}\n        {% for role_row in user.roles %}\n            {% if role_row.role == 'Participant' %}\n                {% if perm_key.update({'has_role': 1 }) %} {% endif %}\n            {% endif %}\n        {% endfor %}\n        {% if perm_key.has_role == 1 %}\n            <div class=\"row\">\n              <div class=\"col-sm-12\">\n                <h3>Tasks</h3>\n                <div style=\"margin-top: 50px;\">\n                    <table class=\"table\">\n                        <thead style=\"background-color: rgb(247, 250, 252); color: rgb(141, 153, 166);\">\n                            <tr>\n                                <th class=\"text-center\">Sl No.</th>\n                                <th class=\"text-center\">ID</th>\n                                <th class=\"text-center\">Subject</th>\n                                <th class=\"text-center\">Project</th>\n                                <th class=\"text-center\">Status</th>\n                                <th class=\"text-center\">View/Submit</th>\n                            </tr>\n                        </thead>\n                        {% set count = {'idx': 0 } %}\n                        {% set task_list = frappe.db.get_list(\"Task\", fields=['name'], order_by='modified desc', page_length='100') %}\n                        {% for task in task_list %}\n                            {% if count.update({'idx': count.idx+1 }) %} {% endif %}\n                            {% set task_doc = frappe.get_doc(\"Task\", task.name) %}\n                            <tr>\n                                <td class=\"text-center\"> {{ count.idx }} </td>\n                                <td class=\"text-center\"> {{ task_doc.name }} </td>\n                                <td class=\"text-left\"> {{ task_doc.subject }} </td>\n                                <td class=\"text-center\"> {{ task_doc.project }} </td>\n                                <td class=\"text-center\"> {{ task_doc.status }} </td>\n                                <td class=\"text-center\">\n                                    <a href=\"/task-view/{{task_doc.name}}\">\n                                        <button>View Task</button>\n                                    </a>\n                                </td>\n                            </tr>\n                        {% endfor %}\n                    </table>\n                </div>\n            </div>\n        {% else %}\n            {% if user.name == 'Guest' %}\n                <div class=\"row\">\n                    <div class=\"page-card\">\n                    \t<h5 class=\"page-card-head\">\n                    \t\t<span class=\"indicator red\">\n                    \t\t\tNot Permitted</span>\n                    \t</h5>\n                    \t<div class=\"page-card-body\">\n                    \t\t<p>You have been logged out. Please Login to continue</p>\n                    \t\t<div class=\"text-center\">\n                    \t\t    <a href=\"/login\" class=\"btn btn-primary btn-sm\">Login</a>\n                    \t\t</div>\n                    \t</div>\n                    </div>\n                </div>\n            {% else %}\n                <div class=\"row\">\n                    <div class=\"page-card\">\n                    \t<h5 class=\"page-card-head\">\n                    \t\t<span class=\"indicator red\">\n                    \t\t\tNot Permitted</span>\n                    \t</h5>\n                    \t<div class=\"page-card-body\">\n                    \t\t<p>Only Participants are allowed to view and submit Tasks. Please contact System Manager.</p>\n                    \t\t<div class=\"text-center\">\n                    \t\t    <a href=\"/events\" class=\"btn btn-primary btn-sm\">Events</a>\n                    \t\t</div>\n                    \t</div>\n                    </div>\n                </div>\n            {% endif %}\n        {% endif %}\n    </body>\n</html>\n",
