  "javascript": "new MultiSelectTag('tools');\n\nfrappe.ready(function() {\n    var $form = $(\"form[id='frmTask']\");\n\n    $form.on(\"change\", \"[type='file']\", function() {\n      var $input = $(this);\n      var input = $input.get(0);\n      \n      if(input.files.length) {\n        input.filedata = { \"files_data\" : [] }; //Initialize as json array.\n\n        window.file_reading = true;\n\n        $.each(input.files, function(key, value) {\n          setupReader(value, input);\n        });\n\n        window.file_reading = false;\n      }\n    });\n\n    $(\"#btn_upload\").click(function() {\n        var response = \"\";\n        var docname = $('#task').val();\n        var response_type = $('#response-type').val();\n        if(response_type == 'URL'){\n            response = $('#response-url').val();\n        }\n        else if(response_type == 'Text'){\n            response = $('#response-text').val();\n        }\n        else if(response_type == 'Data'){\n            response = $('#response-data').val();\n        }\n        else if(response_type == 'Attachment'){\n            response = $('#response-attachment').val();\n        }\n        if(!response){\n            frappe.throw(__('Response is mandatory'))\n        }\n        var tools_used = $('#tools').val();\n        var filedata = $('#response-attachment').prop('filedata');\n        \n        if(filedata){\n            frappe.call({\n                method: \"hackon.hackon.utils.attach_file_to_doctype\",\n                args: {\"filedata\": filedata, \"doctype\": 'Task', \"docname\": docname },\n                freeze: true,\n                freeze_message: __(\"Upload files...\"),\n                callback: function(r){\n                    if(!r.exc) {\n                        frappe.msgprint(__(\"Files uploaded\"));\n                    } else {\n                        frappe.msgprint(__(\"Files not uploaded. <br /> \" + r.exc));\n                    }\n                }\n            });\n        }\n        \n        frappe.call({\n            method: \"hackon.hackon.utils.submit_task_response\",\n            args: {\"docname\": docname, \"response_type\": response_type, \"response\": response, \"tools_used\": tools_used },\n            freeze: true,\n            freeze_message: __(\"Submiting answers..\"),\n            callback: function(r){\n                if(!r.exc) {\n                    frappe.msgprint(__(\"Answers Submitted\"));\n                } else {\n                    frappe.msgprint(__(\"Answers not Submitted. <br /> \" + r.exc));\n                }\n            }\n        });\n    });\n  });\n\n  function setupReader(file, input) {\n    var name = file.name;\n    var reader = new FileReader();\n    reader.onload = function(e) {\n        input.filedata.files_data.push({\n            \"__file_attachment\": 1,\n            \"filename\": file.name,\n            \"dataurl\": reader.result\n        });\n    };\n    reader.readAsDataURL(file);\n  }",
